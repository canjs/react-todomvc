{
  "name": "todomvc",
  "version": "1.0.0",
  "main": "todomvc.js",
  "scripts": {
    "develop": "done-serve --develop --static --port 8080",
    "build": "npm run eslint && steal-tools build",
    "eslint": "eslint ."
  },
  "dependencies": {
    "can-fixture": "^3.0.0",
    "can-realtime-rest-model": "^1.0.0",
    "can-route": "^4.3.12",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "steal-css": "^1.3.2",
    "ylem": "^2.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "done-serve": "^2.3.0",
    "eslint": "^5.5.0",
    "eslint-plugin-react": "^7.11.1",
    "steal": "^2.1.2",
    "steal-tools": "^2.0.5"
  },
  "steal": {
    "plugins": [
      "steal-css"
    ],
    "babelOptions": {
      "plugins": [
        "transform-class-properties",
        "transform-decorators-legacy"
      ]
    }
  }
}
